<openerp><data>

    <record id="view_intercompany_product_stock_form" model="ir.ui.view">
        <field name="model">intercompany.product.stock</field>
        <field name="arch" type="xml">
            <form string="Supplier Quantities">
                <group colspan="2">
                    <group col="4" colspan="1">
                        <button icon="gtk-cancel" special="cancel" string="OK !"/>
                   </group>
                    <group string="Products" col="4" colspan="2">
                        <field name="line_ids" readonly="1">
                            <tree string="Products">
                                <field name="product_code"/>
                                <field name="product_name"/>
                                <field name="product_qty_available"/>
                                <field name="product_virtual_available" />
                            </tree>
                        </field>
                    </group>
                </group>
            </form>
        </field>
    </record>

</data></openerp>

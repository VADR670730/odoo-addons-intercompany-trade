<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.values" id="customer_warehouse">
            <field name="model_id" ref="purchase.model_purchase_order" />
            <field name="key">default</field>
            <field name="key2" />
            <field name="model">purchase.order</field>
            <field name="company_id" ref="integrated_trade_base.customer_company" />
            <field name="name">warehouse_id</field>
            <field name="value" eval="'I' + str(ref('integrated_trade_stock.customer_stock_warehouse')) + '\n.'" />
        </record>

        <record model="ir.values" id="supplier_shop">
            <field name="model_id" ref="sale.model_sale_order" />
            <field name="key">default</field>
            <field name="key2" />
            <field name="model">sale.order</field>
            <field name="company_id" ref="integrated_trade_base.supplier_company" />
            <field name="name">shop_id</field>
            <field name="value" eval="'I' + str(ref('supplier_sale_shop')) + '\n.'" />
        </record>

    </data>
</openerp>
